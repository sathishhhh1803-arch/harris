
#include <stdio.h>

int main() {
    int r, c;
    scanf("%d%d", & r, & c);
    int arr[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", & arr[i][j]);
        }
    }
    int mi = 0, maxsum = 0;
    for (int i = 0; i < r; i++) {
        int sum = 0;
        for (int j = 0; j < c; j++) {
            sum += arr[i][j];

        }
        if (sum > maxsum)
        {
            maxsum = sum;
            mi = i;
        }
    }

 for (int i = 0; i < r; i++) {
    printf("%d ", arr[mi][i]);
}

}


int a, b;
    scanf("%d%d", & a, & b);
    int arr[a][b];
    for (int i = 0; i < a; i++) {
        for (int j = 0; j < b; j++) {
            scanf("%d", & arr[i][j]);
        }
    }
    int sum = arr[0][0];
    for (int i = 0; i < a; i++) {

        for (int j = 0; j < b; j++) {
            if (sum<arr[i][j])
                sum = arr[i][j];
        }
    }
  printf("%d ", sum);
}

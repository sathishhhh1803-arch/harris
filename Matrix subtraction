#include <stdio.h>

int main() {
    int r, c;
    scanf("%d%d", & r, & c);
    int arr[r][c];
    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            scanf("%d", & arr[i][j]);
        }
    }

    int a, b;
    scanf("%d%d", & a, & b);
    int arr1[a][b];
    for (int i = 0; i < a; i++) {
        for (int j = 0; j < b; j++) {
            scanf("%d", & arr1[i][j]);
        }
    }

    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            printf("%d ", arr[i][j] - arr1[i][j]);

        }
        printf("\n");

    }



}

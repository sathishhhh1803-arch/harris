int a, b;
    scanf("%d%d", & a, & b);
    int arr[a][b];
    for (int i = 0; i < a; i++) {
        for (int j = 0; j < b; j++) {
            scanf("%d", & arr[i][j]);
        }
    }
    
    for (int i = 0; i < a; i++) {

       int sum = arr[i][0];
        for (int j = 0; j < b; j++) {
            if (sum<arr[j][i])
                sum = arr[j][i];
        }
    } printf("%d ", sum);
}
